<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginDisplay</servlet-name>
        <servlet-class>LoginDisplay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insert_stu</servlet-name>
        <servlet-class>insert_stu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Students</servlet-name>
        <servlet-class>Students</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Result</servlet-name>
        <servlet-class>Result</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_mks</servlet-name>
        <servlet-class>update_mks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminforgotsession</servlet-name>
        <servlet-class>adminforgotsession</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginDisplay</servlet-name>
        <url-pattern>/LoginDisplay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insert_stu</servlet-name>
        <url-pattern>/insert_stu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Students</servlet-name>
        <url-pattern>/Students</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Result</servlet-name>
        <url-pattern>/Result</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_mks</servlet-name>
        <url-pattern>/update_mks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminforgotsession</servlet-name>
        <url-pattern>/adminforgotsession</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
